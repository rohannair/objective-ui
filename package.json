{
  "name": "sidewalks",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run build:dll && better-npm-run dev",
    "build:dll": "webpack --config ./config/webpack.dll.js",
    "build:webpack": "better-npm-run build:webpack",
    "build": "npm run clean && npm run build:dll && npm run build:webpack"
  },
  "betterScripts": {
    "build:webpack": {
      "command": "webpack -p --config ./config/webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "nodemon ./bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    }
  },
  "private": true,
