{
  "name": "objective-ui",
  "version": "0.5.0",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "dev": "better-npm-run dev",
    "build:dll": "webpack --config ./config/webpack.dll.js",
    "build:webpack": "better-npm-run build:webpack",
    "build": "npm run clean && npm run build:webpack",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js src",
    "lint:js:fix": "eslint --ext .js src --fix",
    "lint:css": "stylelint src/**/*.css",
    "new:comp": "babel-node ./scripts/newComponent.js",
    "new:cont": "babel-node ./scripts/newContainer.js",
    "test:watch": "npm test -- --watch"
  },
  "betterScripts": {
    "build:webpack": {
      "command": "webpack -p --config config/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "./node_modules/.bin/webpack-dashboard -- ./node_modules/.bin/webpack-dev-server --config config/webpack.config.dev.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    }
  },
  "private": true,
