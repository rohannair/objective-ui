{
  "name": "sidewalks",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "better-npm-run dev",
    "build:dll": "webpack --config ./config/webpack.dll.js",
    "build:webpack": "better-npm-run build:webpack",
    "build": "npm run clean && npm run build:dll && npm run build:webpack",
    "lint": "eslint --ext .js src",
    "lint:fix": "eslint --ext .js src --fix"
  },
  "betterScripts": {
    "build:webpack": {
      "command": "webpack -p",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "nodemon ./bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*,webpack:*"
      }
    }
  },
