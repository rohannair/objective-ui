    "build:webpack": {
      "command": "webpack -p",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "nodemon ./bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*,webpack:*,koa-generic-session:*"
      }
    }
