  },
  "jest": {
    "setupFiles": [
      "./scripts/jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/codemods/"
    ]
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
